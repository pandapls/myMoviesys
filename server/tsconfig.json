{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "outDir": "./dist",
    "emitDecoratorMetadata": true,
    "strictNullChecks": true,
    "removeComments": true, // 移除掉注释
    "noEmitOnError": true, // 发生错误不生成结果
    "lib": ["ESNext"],
    "experimentalDecorators": true,
    "esModuleInterop": true
  },
  "include": ["./src"]
}
